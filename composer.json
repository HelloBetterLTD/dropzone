{
  "name": "silverstripe/dropzone",
  "type": "silverstripe-vendormodule",
  "description": "dropzone for silverstripe",
  "require": {
    "silverstripe/asset-admin": "^1.1",
    "silverstripe/assets": "^1.1",
    "silverstripe/config": "^1",
    "silverstripe/admin": "@stable",
    "silverstripe/framework": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Silverstripe\\Dropzone\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "expose": [
      "client/dist"
    ],
    "installer-name": "silverstripedropzone"
  }
}
